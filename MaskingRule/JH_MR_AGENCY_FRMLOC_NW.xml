  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_AGENCY_FRMLOC_NW">
    <RulePort dataType="String" order="0" scale="40" precision="40" length="0" mandatory="true" portType="INPUT" name="Input1"/>
    <RulePort dataType="String" order="10" scale="40" precision="40" length="0" mandatory="true" portType="OUTPUT" expression="V_match" name="Output1"/>
    <RulePort dataType="String" order="6" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(REG_MATCH(RTRIM(LTRIM(SUBSTR(Input1,2,3))),'(\d\d\d)')=TRUE,'X','Y')" name="V_Regmatch"/>
    <RulePort dataType="String" order="8" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(REPLACESTR(1,V_fullname,',',''))" name="Upper_fullname"/>
    <RulePort dataType="String" inputPortName="Input1" associatedRuleName="JH_MR_Full_Name" order="7" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="V_fullname"/>
    <RulePort dataType="String" order="9" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(V_Regmatch='Y',Upper_fullname,V_concat)" name="V_match"/>
    <RulePort dataType="String" order="1" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Input1,5,(length(Input1)-4))" name="V_extract"/>
    <RulePort dataType="String" order="4" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="upper(REPLACESTR(1,V_org,',',''))" name="Upper_org"/>
    <RulePort dataType="String" order="5" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(V_extractnum,' '),Upper_org)" name="V_concat"/>
    <RulePort dataType="String" order="2" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Input1,1,4)" name="V_extractnum"/>
    <RulePort dataType="String" inputPortName="V_extract" associatedRuleName="JH_MR_Org_Name" order="3" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="V_org"/>
  </Rule>
