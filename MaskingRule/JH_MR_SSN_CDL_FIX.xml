<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_SSN_CDL_FIX" description="using decode logic to mask the SSN">
    <RulePort dataType="String" order="12" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(length(Input_SSN)=4,Concat4,Concatinate)" name="Matching"/>
    <RulePort dataType="String" order="6" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(DECODE(TO_INTEGER(substr(Input_SSN,6,1)),1,9,2,8,3,7,4,6,5,1,6,2,7,0,8,4,9,5,0,3))" name="Digit6th"/>
    <RulePort dataType="String" order="13" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(length(RTRIM(LTRIM(Input_SSN)))=0 or Input_SSN = '0',Input_SSN,Matching)" name="FinalMatch"/>
    <RulePort dataType="String" order="14" scale="10" precision="10" length="0" mandatory="true" portType="OUTPUT" expression="FinalMatch" name="Outputport"/>
