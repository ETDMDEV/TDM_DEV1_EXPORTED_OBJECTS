    <RulePort dataType="String" order="14" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="VAR_REST_FORMAT3||VAR_DECODE_FORMAT3" name="VAR_CONCAT_FORMAT3"/>
    <RulePort dataType="String" order="7" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(IN_DOB,9,2)))" name="VAR_DD_FORMAT3"/>
    <RulePort dataType="String" order="10" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(DECODE(TO_INTEGER(VAR_DD_FORMAT2),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00))" name="VAR_DD_DECODE_FORMAT2"/>
    <RulePort dataType="String" order="8" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(IN_DOB,1,8)))" name="VAR_REST_FORMAT3"/>
    <RulePort dataType="String" order="0" scale="255" precision="255" length="0" mandatory="true" portType="INPUT" name="INPUT_DOB"/>
    <RulePort dataType="String" order="16" scale="255" precision="255" length="0" mandatory="true" portType="OUTPUT" expression="IIF(LENGTH(LTRIM(RTRIM(INPUT_DOB)))=0 OR ISNULL(INPUT_DOB),INPUT_DOB,FINAL_DOB)" name="OUTPUT_DOB"/>
    <RulePort dataType="String" order="12" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(VAR_DD_DECODE_FORMAT1,'-'),VAR_REST_FORMAT1)" name="VAR_CONCAT_FORMAT1"/>
