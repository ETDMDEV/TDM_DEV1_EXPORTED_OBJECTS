    <RulePort dataType="String" order="6" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(IN_DOB,INSTR(IN_DOB,',',1,1)+1,LENGTH(IN_DOB))))" name="VAR_YY_FORMAT2"/>
    <RulePort dataType="String" order="3" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(IN_DOB,INSTR(IN_DOB,'-',1,1)+1,LENGTH(IN_DOB))))" name="VAR_REST_FORMAT1"/>
    <RulePort dataType="String" order="4" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(IN_DOB,INSTR(IN_DOB,' ',1,1)+1,INSTR(IN_DOB,',',1,1)-INSTR(IN_DOB,' ',1,1)+1)))" name="VAR_DD_FORMAT2"/>
    <RulePort dataType="String" order="13" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(CONCAT(CONCAT(CONCAT(VAR_MM_FORMAT2,' '),VAR_DD_DECODE_FORMAT2),','),' '),VAR_YY_FORMAT2)" name="VAR_CONCAT_FORMAT2"/>
