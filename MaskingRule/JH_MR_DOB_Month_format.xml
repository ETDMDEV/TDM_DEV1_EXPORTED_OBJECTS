<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_Month_format" description="To mask date as Month dd, yyyy">
    <RulePort dataType="Numeric" order="4" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LENGTH(V_DOB)" name="V_LEN"/>
    <RulePort dataType="String" order="6" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(V_DOB,V_DOB_SP,V_DOB_CM-V_DOB_SP)" name="V_DOB_D"/>
    <RulePort dataType="String" order="11" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="iif(ISNULL(IN_DOB) OR TO_INTEGER(LENGTH(LTRIM(RTRIM(IN_DOB))))=0,IN_DOB,V_CONCAT)" name="V_DOB_FINAL"/>
    <RulePort dataType="String" order="1" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(IN_DOB))" name="V_DOB"/>
    <RulePort dataType="String" order="10" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="V_DOB_M||V_DOB_DECODE||V_DOB_YY" name="V_CONCAT"/>
    <RulePort dataType="String" order="0" scale="30" precision="30" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="String" order="9" scale="10" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(V_DOB_DECODE,2,'0')" name="LPAD_DD"/>
    <RulePort dataType="Numeric" order="2" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(V_DOB,' ',1)" name="V_DOB_SP"/>
    <RulePort dataType="Numeric" order="3" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(IN_DOB,',')" name="V_DOB_CM"/>
    <RulePort dataType="String" order="7" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(V_DOB,V_DOB_CM,(V_LEN+1)-V_DOB_CM)" name="V_DOB_YY"/>
    <RulePort dataType="String" order="12" scale="30" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="V_DOB_FINAL" name="O_CONCAT"/>
    <RulePort dataType="String" order="5" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(V_DOB,1,V_DOB_SP)" name="V_DOB_M"/>
    <RulePort dataType="String" order="8" scale="10" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(DECODE(TO_INTEGER(V_DOB_D),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00))" name="V_DOB_DECODE"/>
  </Rule>
</Metadata>