  </Rule>
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_TRANSIT_NO_NEW">
    <RulePort dataType="Numeric" order="7" scale="14" precision="9" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DECIMAL(CONCAT((ADD_L),'0'))" name="MV_WTTL_0_UNIT"/>
    <RulePort dataType="Numeric" order="3" scale="14" precision="1" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DECIMAL((TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,1,1))*3)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,2,1))*7)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,3,1))*1)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,4,1))*3)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,5,1))*7)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,6,1))*1)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,7,1))*3)+(TO_DECIMAL(SUBSTR(MASK_TRANSIT_NO,8,1))*7))" name="WHASH"/>
    <RulePort dataType="String" order="9" scale="15" precision="15" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="concat(substr(MASK_TRANSIT_NO,1,8),TO_CHAR(COMPUT_WTTL))" name="VAR_AGENCY_TRANSIT_NO"/>
    <RulePort dataType="String" order="6" scale="15" precision="15" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(TO_CHAR(ADD_WTTL),1,length(TO_CHAR(ADD_WTTL))-1)" name="ADD_L"/>
    <RulePort dataType="String" inputPortName="Agency_transit_no" associatedRuleName="JH_MR_SSN" order="2" scale="15" precision="15" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_TRANSIT_NO"/>
    <RulePort dataType="Numeric" order="4" scale="15" precision="1" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="WHASH" name="WTTL"/>
    <RulePort dataType="Numeric" order="5" scale="14" precision="1" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="(WTTL+10)" name="ADD_WTTL"/>
    <RulePort dataType="Numeric" order="0" scale="15" precision="9" length="0" mandatory="true" portType="INPUT" name="Agency_transit_no"/>
    <RulePort dataType="Numeric" order="11" scale="14" precision="10" length="0" mandatory="true" portType="OUTPUT" expression="VAR_NUM_TRANSIT_NO" name="Out_Transit_no"/>
    <RulePort dataType="Numeric" order="8" scale="14" precision="9" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="(TO_DECIMAL(MV_WTTL_0_UNIT)-WHASH)" name="COMPUT_WTTL"/>
    <RulePort dataType="Numeric" order="10" scale="14" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DECIMAL(VAR_AGENCY_TRANSIT_NO)" name="VAR_NUM_TRANSIT_NO"/>
    <RulePort dataType="String" order="1" scale="15" precision="15" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(Agency_transit_no)" name="CHAR_TRANSIT_NO"/>
  </Rule>
</Metadata>