<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="true" name="JH_MR_SSN" description="JH_Key_Masking" dataType="String" maskingType="KEY">
    <RuleParameter name="excp_nullspacetreat" value="false"/>
    <RuleParameter name="errhandling_const" value="false"/>
    <RuleParameter name="keyStr_maskFormat"/>
    <RuleParameter name="excp_nullspaceconst" value="false"/>
    <RuleParameter name="excp_nullspacelogerr" value="false"/>
    <RuleParameter name="excp_nullspaceignore" value="true"/>
    <RuleParameter name="keyStr_srcCombo" value="Mask_Only"/>
    <RuleParameter name="keyStr_resCombo" value="Use_Only"/>
    <RuleParameter name="keyStr_src_chk" value="true"/>
    <RuleParameter name="errhandling_errout" value="false"/>
    <RuleParameter name="errhandling_ignore" value="false"/>
    <RuleParameter name="keyStr_srcChars" value="0123456789"/>
    <RuleParameter name="postprocess_expression"/>
    <RuleParameter name="keyStr_resChars" value="0123456789"/>
    <RuleParameter name="keyStr_seed" value="12"/>
    <RuleParameter name="preprocess_expression"/>
    <RuleParameter name="errhandling_defser" value="true"/>
    <RuleParameter name="excp_trim" value="true"/>
    <RuleParameter name="seed_value_is_param" value="false"/>
    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="keyStr_res_chk" value="true"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
  </Rule>
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_SSN_10DIGIT_FIX" description="This rule is used to mask ssn of 10 length">
    <RulePort dataType="Numeric" order="2" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_INTEGER(LENGTH(V_SSN))" name="LEN_SSN"/>
    <RulePort dataType="Numeric" order="15" scale="10" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="V_SSN_FINAL" name="OUT_SSN"/>
    <RulePort dataType="String" inputPortName="V_TRIM_R" associatedRuleName="JH_MR_SSN" order="8" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="VMASK_RIGHT"/>
    <RulePort dataType="Numeric" order="13" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LEN_SSN&lt;10,V_INTEGER,V_TO_INT)" name="V_MATCH"/>
    <RulePort dataType="String" inputPortName="V_LPAD_9" associatedRuleName="JH_MR_SSN" order="11" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="V_MASK"/>
    <RulePort dataType="String" order="7" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(V_LEFT))" name="VTRIM_L"/>
    <RulePort dataType="String" order="1" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(ISNULL(TO_CHAR(IN_SSN)),'999999999',TO_CHAR(IN_SSN))" name="V_SSN"/>
    <RulePort dataType="Numeric" order="0" scale="10" precision="30" length="0" mandatory="true" portType="INPUT" name="IN_SSN"/>
    <RulePort dataType="String" order="3" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(V_SSN,1,LEN_SSN-9)" name="V_LEFT"/>
    <RulePort dataType="String" order="4" scale="10" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(V_SSN,LEN_SSN-8,9)" name="V_RIGHT"/>
    <RulePort dataType="Numeric" order="10" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="to_decimal(V_CONCAT,TRUE)" name="V_TO_INT"/>
    <RulePort dataType="String" order="9" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(VTRIM_L,VMASK_RIGHT)" name="V_CONCAT"/>
    <RulePort dataType="Numeric" order="14" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(ISNULL(IN_SSN) OR TO_DECIMAL(IN_SSN)=0,IN_SSN,V_MATCH)" name="V_SSN_FINAL"/>
    <RulePort dataType="String" order="5" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(V_RIGHT))" name="V_TRIM_R"/>
    <RulePort dataType="Numeric" order="12" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DECIMAL(V_MASK,TRUE)" name="V_INTEGER"/>
    <RulePort dataType="String" order="6" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(LTRIM(RTRIM(V_SSN)),9,'0')" name="V_LPAD_9"/>
  </Rule>
</Metadata>