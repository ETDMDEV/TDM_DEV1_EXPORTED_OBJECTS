<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="true" name="JH_MR_DOB" description="JH_Random_Bluring_Masking" dataType="Date" maskingType="RANDOM">
    <RuleParameter name="errhandling_defser" value="true"/>
    <RuleParameter name="errhandling_const" value="false"/>
    <RuleParameter name="preprocess_expression"/>
    <RuleParameter name="excp_nullspaceconst" value="false"/>
    <RuleParameter name="errhandling_errout" value="false"/>
    <RuleParameter name="excp_nullspacelogerr" value="false"/>
    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="postprocess_expression"/>
    <RuleParameter name="excp_nullspaceignore" value="true"/>
    <RuleParameter name="errhandling_ignore" value="false"/>
    <RuleParameter name="randDate_minblur" value="10"/>
    <RuleParameter name="randDate_range" value="false"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
    <RuleParameter name="randDate_BlurUnit" value="Day"/>
    <RuleParameter name="randDate_maxblur" value="10"/>
    <RuleParameter name="randDate_blurring" value="true"/>
    <RuleParameter name="excp_nullspacetreat" value="false"/>
  </Rule>
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_INT_WITHZERO" description="For date in YYYYMMDD integer data type">
    <RulePort dataType="Date" order="2" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DATE(TO_CHAR(IN_VALUE), 'YYYYMMDD')" name="VAR_IN_DOB"/>
    <RulePort dataType="Numeric" order="4" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_INTEGER(TO_CHAR(VAR_OUT_DOB,'YYYYMMDD'),TRUE)" name="VAR_OUT_NUMERIC"/>
    <RulePort dataType="String" order="1" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(IN_VALUE)" name="VAR_DOB_CHECK"/>
    <RulePort dataType="Numeric" order="0" scale="8" precision="10" length="0" mandatory="true" portType="INPUT" name="IN_VALUE"/>
    <RulePort dataType="Date" qualifier="IS_DATE(VAR_DOB_CHECK,'YYYYMMDD')=1" inputPortName="VAR_IN_DOB" associatedRuleName="JH_MR_DOB" order="3" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="VAR_OUT_DOB"/>
    <RulePort dataType="Numeric" order="5" scale="8" precision="10" length="0" mandatory="true" portType="OUTPUT" expression="VAR_OUT_NUMERIC" name="OUT_INT_DOB"/>
  </Rule>
</Metadata>