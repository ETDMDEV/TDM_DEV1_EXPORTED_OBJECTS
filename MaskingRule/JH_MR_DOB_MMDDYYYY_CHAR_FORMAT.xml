<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_MMDDYYYY_CHAR_FORMAT">
    <RulePort dataType="String" order="1" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(length(LTRIM(RTRIM(IN_DOB)))=0 OR IN_DOB='00000000' OR ISNULL(IN_DOB),'11111111',IN_DOB)" name="VAR1"/>
    <RulePort dataType="String" order="5" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(DECODE(TO_INTEGER(VAR_DD),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00)),2,0)" name="VAR_DD_DECODE"/>
    <RulePort dataType="String" order="6" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(VAR_MM,VAR_DD_DECODE),VAR_YYYY)" name="VAR_DOB_CONCAT"/>
    <RulePort dataType="String" order="3" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,3,2)" name="VAR_DD"/>
    <RulePort dataType="String" order="8" scale="8" precision="8" length="0" mandatory="true" portType="OUTPUT" expression="VAR_DOB_FINAL" name="OUT_DOB"/>
    <RulePort dataType="String" order="0" scale="8" precision="8" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="String" order="7" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(length(LTRIM(RTRIM(IN_DOB)))=0 OR IN_DOB='00000000' OR ISNULL(IN_DOB),IN_DOB,VAR_DOB_CONCAT)" name="VAR_DOB_FINAL"/>
    <RulePort dataType="String" order="4" scale="4" precision="4" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,5,4)" name="VAR_YYYY"/>
    <RulePort dataType="String" order="2" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,1,2)" name="VAR_MM"/>
  </Rule>
</Metadata>