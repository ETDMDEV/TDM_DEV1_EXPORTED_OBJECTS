<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="true" name="JH_MR_DOB" description="JH_Random_Bluring_Masking" dataType="Date" maskingType="RANDOM">
    <RuleParameter name="errhandling_defser" value="true"/>
    <RuleParameter name="errhandling_const" value="false"/>
    <RuleParameter name="preprocess_expression"/>
    <RuleParameter name="excp_nullspaceconst" value="false"/>
    <RuleParameter name="errhandling_errout" value="false"/>
    <RuleParameter name="excp_nullspacelogerr" value="false"/>
    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="postprocess_expression"/>
    <RuleParameter name="excp_nullspaceignore" value="true"/>
    <RuleParameter name="errhandling_ignore" value="false"/>
    <RuleParameter name="randDate_minblur" value="10"/>
    <RuleParameter name="randDate_range" value="false"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
    <RuleParameter name="randDate_BlurUnit" value="Day"/>
    <RuleParameter name="randDate_maxblur" value="10"/>
    <RuleParameter name="randDate_blurring" value="true"/>
    <RuleParameter name="excp_nullspacetreat" value="false"/>
  </Rule>
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_YYYYMMDD_FORMAT" description="Date in YYYYMMDD format">
    <RulePort dataType="String" order="5" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0,IN_DOB,TO_CHAR(Var_mask,'YYYYMMDD'))" name="Var_final"/>
    <RulePort dataType="String" order="0" scale="8" precision="8" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="Date" inputPortName="Var2" associatedRuleName="JH_MR_DOB" order="4" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Var_mask"/>
    <RulePort dataType="String" order="6" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(TO_INTEGER(IN_DOB)=0,IN_DOB,Var_final)" name="Finally"/>
    <RulePort dataType="Date" order="3" scale="8" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DATE(var3,'YYYYMMDD')" name="Var2"/>
    <RulePort dataType="String" order="1" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0,'11111111',IN_DOB)" name="Var1"/>
    <RulePort dataType="String" order="7" scale="8" precision="8" length="0" mandatory="true" portType="OUTPUT" expression="Finally" name="OUT_DOB"/>
    <RulePort dataType="String" order="2" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(SUBSTR(IN_DOB,7,2)='00','19931102',Var1)" name="var3"/>
  </Rule>
</Metadata>