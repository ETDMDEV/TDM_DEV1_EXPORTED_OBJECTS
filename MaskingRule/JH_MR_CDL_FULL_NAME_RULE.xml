  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_CDL_FULL_NAME_RULE" description="Split Full_Name and mask first,mid and last name">
    <RulePort dataType="String" order="3" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(SUBSTR(IN_NAME,INSTR(IN_NAME,' ',1,1),LENGTH(IN_NAME)))" name="VAR_REST"/>
    <RulePort dataType="String" order="2" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(IN_NAME,1,INSTR(IN_NAME,' ',1,1)-1)" name="VAR_FIRST"/>
    <RulePort dataType="String" order="12" scale="80" precision="80" length="0" mandatory="true" portType="OUTPUT" expression="IIF(LENGTH(LTRIM(RTRIM(IN_NAME)))=0 OR ISNULL(IN_NAME),IN_NAME,CONCAT_NAME)" name="OUT_NAME"/>
    <RulePort dataType="String" order="7" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(VAR_MID))" name="VAR_MID_TRIM"/>
    <RulePort dataType="String" inputPortName="VAR_MID_TRIM" associatedRuleName="JH_MR_MIDDLE_INITIAL" order="9" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_MID"/>
    <RulePort dataType="String" inputPortName="VAR_FIRST" associatedRuleName="JH_MR_First_Name" order="8" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_FIRST"/>
    <RulePort dataType="String" order="6" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(VAR_LAST))" name="VAR_LAST_TRIM"/>
    <RulePort dataType="String" order="5" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(VAR_REST,' ')>0,SUBSTR(VAR_REST,INSTR(VAR_REST,' ',1,1),LENGTH(VAR_REST)),VAR_REST)" name="VAR_LAST"/>
    <RulePort dataType="String" order="4" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(VAR_REST,' ')>0,SUBSTR(VAR_REST,1,INSTR(VAR_REST,' ',1,1)-1),'')" name="VAR_MID"/>
    <RulePort dataType="String" order="0" scale="80" precision="80" length="0" mandatory="true" portType="INPUT" name="IN_NAME"/>
    <RulePort dataType="String" order="1" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(IN_NAME))" name="VAR_IN_NAME"/>
    <RulePort dataType="String" order="11" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(CONCAT(CONCAT(CONCAT(CONCAT(MASK_FIRST,' '),MASK_MID),' '),MASK_LAST))" name="CONCAT_NAME"/>
    <RulePort dataType="String" inputPortName="VAR_LAST_TRIM" associatedRuleName="JH_MR_Last_Name" order="10" scale="80" precision="80" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_LAST"/>
  </Rule>
