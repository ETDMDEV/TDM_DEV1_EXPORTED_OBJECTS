<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_MM_DD_YYYY_FORMAT_GLTC" description="DOB IN M/DD/YYYY &amp; MM/DD/YYYY FORMAT FOR TIMESTAMP DATATYPE">
    <RulePort dataType="String" order="3" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(CONVERT1,(INSTR(CONVERT1,'/',1,1)+1),(INSTR(CONVERT1,'/',1,2)-1))" name="VAR_DD"/>
    <RulePort dataType="String" order="1" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(SUBSTR(LTRIM(RTRIM(TO_CHAR(DOB_IN))),1,10))=10,SUBSTR(TO_CHAR(DOB_IN),1,10),'1/11/1111')" name="CONVERT1"/>
    <RulePort dataType="Date" order="7" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(CONVERT1)=10,TO_DATE(VAR_CONCAT,'MM/DD/YYYY'),DOB_IN)" name="CONVERTBACK"/>
    <RulePort dataType="String" order="6" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(VAR_MM,CONCAT('/',CONCAT(VAR_DD_MASK,CONCAT('/',VAR_YY))))" name="VAR_CONCAT"/>
    <RulePort dataType="Date" order="8" scale="29" precision="29" length="0" mandatory="true" portType="OUTPUT" expression="CONVERTBACK" name="DOB_OUT"/>
    <RulePort dataType="String" order="2" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(SUBSTR(CONVERT1,1,(INSTR(CONVERT1,'/',1,1)-1)),2,'0')" name="VAR_MM"/>
    <RulePort dataType="Date" order="0" scale="29" precision="29" length="0" mandatory="true" portType="INPUT" name="DOB_IN"/>
    <RulePort dataType="String" order="5" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(DECODE(TO_INTEGER(VAR_DD),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00),2,'0')" name="VAR_DD_MASK"/>
    <RulePort dataType="String" order="4" scale="29" precision="29" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(CONVERT1,(INSTR(CONVERT1,'/',1,2)+1),LENGTH(CONVERT1))" name="VAR_YY"/>
  </Rule>
</Metadata>