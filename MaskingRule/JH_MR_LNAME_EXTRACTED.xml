<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_LNAME_EXTRACTED" description="Extract LASTNAME from FullName field.&#xa;Format of full name : FNAME&lt;space>MNAME&lt;space>LNAME">
    <RulePort dataType="String" order="0" scale="25" precision="25" length="0" mandatory="true" portType="INPUT" name="FULLNAME"/>
    <RulePort dataType="String" order="2" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(V_FULLNAME,INSTR(V_FULLNAME,CHR(32),-1,1),Length(V_FULLNAME))))" name="V_LASTNAME"/>
    <RulePort dataType="String" order="1" scale="25" precision="25" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="FULLNAME" name="V_FULLNAME"/>
    <RulePort dataType="String" order="3" scale="20" precision="20" length="0" mandatory="true" portType="OUTPUT" expression="V_LASTNAME" name="LASTNAME"/>
  </Rule>
</Metadata>