<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_YYYY_MM_DD_Timestamp_OR_YYYYMMDD_FORMAT" description="Masking of date columns which contains the date in two different date formats &quot;yyyy-mm-dd 00:00:00.000&quot; &amp; &quot;yyyymmdd&quot; in a single column">
    <RulePort dataType="String" order="7" scale="6" precision="6" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,1,6)" name="VAR_YYYYMM2"/>
    <RulePort dataType="String" order="11" scale="23" precision="23" length="0" mandatory="true" portType="OUTPUT" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0 OR ISNULL(IN_DOB),IN_DOB,FINAL_DATE)" name="OUT_DOB"/>
    <RulePort dataType="String" order="6" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,1,8)" name="VAR_YYYYMM1"/>
    <RulePort dataType="String" order="5" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(DECODE(TO_INTEGER(VAR_DD2),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00)),2,'0')" name="VAR_DD_MASK2"/>
    <RulePort dataType="String" order="10" scale="23" precision="23" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(VAR1,'-')>0,CONCAT_DATE1,CONCAT_DATE2)" name="FINAL_DATE"/>
    <RulePort dataType="String" order="1" scale="23" precision="23" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0 OR ISNULL(IN_DOB),'1111-11-11',IN_DOB)" name="VAR1"/>
    <RulePort dataType="String" order="8" scale="23" precision="23" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="VAR_YYYYMM1||VAR_DD_MASK1||' '||'00:00:00.000'" name="CONCAT_DATE1"/>
    <RulePort dataType="String" order="2" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,9,10)" name="VAR_DD1"/>
    <RulePort dataType="String" order="4" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(DECODE(TO_INTEGER(VAR_DD1),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00)),2,'0')" name="VAR_DD_MASK1"/>
    <RulePort dataType="String" order="3" scale="2" precision="2" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,7,8)" name="VAR_DD2"/>
    <RulePort dataType="String" order="0" scale="23" precision="23" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="String" order="9" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="VAR_YYYYMM2||VAR_DD_MASK2" name="CONCAT_DATE2"/>
  </Rule>
</Metadata>