  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_AGENCY_SHORTNAME">
    <RulePort dataType="String" order="3" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(GIVENNM)))=0 or ISNULL(GIVENNM),SURNM,nameCONCAT)" name="MATCH"/>
