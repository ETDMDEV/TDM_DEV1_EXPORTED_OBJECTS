  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_CSC_DOB_2" description="FORMATS: January 4,1968 &#xa;&amp;&#xa;1945-02-09">
    <RulePort dataType="String" order="3" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(trimm,INSTR(trimm,',',1,1)+1,LENGTH(trimm))))" name="Year1"/>
