    <RulePort dataType="String" order="5" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(DECODE(TO_INTEGER(Date1),1,11,2,12,3,13,4,14,5,15,6,16,7,17,8,18,9,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,1,20,2,21,3,22,4,23,5,24,6,25,7,26,8,27,9,28,10,29,21,30,22,31,23,00,00))" name="Maskdecode1"/>
    <RulePort dataType="String" order="10" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(Restdate2,Maskdecode2)" name="Concat2"/>
    <RulePort dataType="String" order="4" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(trimm,INSTR(trimm,' ',1,1)+1,INSTR(trimm,',',1,1)-INSTR(trimm,' ',1,1)+1)))" name="Date1"/>
    <RulePort dataType="String" order="12" scale="255" precision="255" length="0" mandatory="true" portType="OUTPUT" expression="IIF(LENGTH(LTRIM(RTRIM(InDOB)))=0 OR ISNULL(InDOB),InDOB,Match)" name="OutDOB"/>
