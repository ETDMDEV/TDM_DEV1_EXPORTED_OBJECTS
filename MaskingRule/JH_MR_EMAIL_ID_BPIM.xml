<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="true" name="JH_MR_ALPHABETS" dataType="String" maskingType="KEY">
    <RuleParameter name="excp_nullspacetreat" value="false"/>
    <RuleParameter name="keyStr_srcChars" value="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"/>
    <RuleParameter name="keyStr_maskFormat"/>
    <RuleParameter name="errhandling_const" value="false"/>
    <RuleParameter name="keyStr_seed" value="12"/>
    <RuleParameter name="excp_nullspaceconst" value="false"/>
    <RuleParameter name="keyStr_srcCombo" value="Mask_Only"/>
    <RuleParameter name="keyStr_src_chk" value="true"/>
    <RuleParameter name="seed_value_is_param" value="false"/>
    <RuleParameter name="keyStr_resCombo" value="Use_Only"/>
    <RuleParameter name="keyStr_resChars" value="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"/>
    <RuleParameter name="excp_nullspacelogerr" value="false"/>
    <RuleParameter name="postprocess_expression" value="UPPER(Input_Column)"/>
    <RuleParameter name="errhandling_errout" value="false"/>
    <RuleParameter name="preprocess_expression" value="UPPER(Input_Column)"/>
    <RuleParameter name="errhandling_ignore" value="false"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
    <RuleParameter name="keyStr_res_chk" value="true"/>
    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="errhandling_defser" value="true"/>
