  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_EMAIL_ID_BPIM" description="Rule created to retain the domain name.">
    <RulePort dataType="String" inputPortName="V_LEFT" associatedRuleName="JH_MR_ALPHABETS" order="5" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="M_EMAIL"/>
    <RulePort dataType="String" order="7" scale="10" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="CONCAT_EMAIL" name="O_EMAIL"/>
    <RulePort dataType="Numeric" order="1" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(I_EMAIL,'@')" name="V_PART"/>
    <RulePort dataType="String" order="6" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LOWER(M_EMAIL)||LOWER(V_RIGHT)" name="CONCAT_EMAIL"/>
    <RulePort dataType="String" order="4" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(I_EMAIL,V_PART,(V_LEN-V_PART))" name="V_RIGHT"/>
    <RulePort dataType="String" order="0" scale="10" precision="30" length="0" mandatory="true" portType="INPUT" name="I_EMAIL"/>
    <RulePort dataType="String" order="3" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(I_EMAIL,1,V_PART-1)" name="V_LEFT"/>
    <RulePort dataType="Numeric" order="2" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LENGTH(I_EMAIL)+1" name="V_LEN"/>
  </Rule>
</Metadata>