  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_AGENCY_SHORTNAME_NEW">
    <RulePort dataType="String" order="1" scale="30" precision="30" length="0" mandatory="true" portType="INPUT" name="Shortname"/>
    <RulePort dataType="String" inputPortName="Org" associatedRuleName="JH_MR_Org_Name" order="7" scale="30" precision="30" length="0" mandatory="false" portType="LOCAL_VARIABLE" expression="Org" name="Upper_org"/>
    <RulePort dataType="String" order="2" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Shortname,(INSTR(Shortname,',')+2),(length(Shortname)-(INSTR(Shortname,' ')-1)))" name="first"/>
    <RulePort dataType="String" inputPortName="first_1" associatedRuleName="JH_MR_First_Name" order="4" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="firstname_rule"/>
    <RulePort dataType="String" order="11" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(Upper_last,', '),Upper_first)" name="Fullname"/>
    <RulePort dataType="String" order="15" scale="30" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="matching" name="Final_data"/>
    <RulePort dataType="String" order="5" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(REPLACESTR(1,firstname_rule,',',''))" name="Upper_first"/>
    <RulePort dataType="String" order="12" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(Givenname)))=0,Upper_org,Fullname)" name="Match"/>
    <RulePort dataType="String" inputPortName="Org" associatedRuleName="JH_MR_Org_Name" order="13" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Upper_Organization"/>
    <RulePort dataType="String" inputPortName="Givenname" associatedRuleName="JH_MR_Last_Name" order="9" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="lastname_rule"/>
    <RulePort dataType="String" order="8" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Shortname,1,INSTR(Shortname,',')-1)" name="last"/>
    <RulePort dataType="String" order="10" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(REPLACESTR(1,lastname_rule,',',''))" name="Upper_last"/>
    <RulePort dataType="String" order="14" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(Givenname)))=0,Upper_Organization,Fullname)" name="matching"/>
    <RulePort dataType="String" inputPortName="Shortname" associatedRuleName="JH_MR_Org_Name" order="6" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="upper(REPLACESTR(1,Shortname,',',''))" name="Org"/>
