  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_CDL_FIRST_NAME_RULE" description="FIRST_NM MID_INITIAL">
    <RulePort dataType="String" order="14" scale="40" precision="40" length="0" mandatory="true" portType="OUTPUT" expression="FINAL_LAST_NAME" name="OUT_LAST_NAME"/>
    <RulePort dataType="String" order="12" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(MASK_LAST_NAME)" name="FINAL_LAST_NAME"/>
    <RulePort dataType="String" order="4" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(IN_NAME,' ')>1,SUBSTR(IN_NAME,INSTR(IN_NAME,' ')+1,LENGTH(IN_NAME)-INSTR(IN_NAME,' ')),' ')" name="IN_MID_INITIAL"/>
    <RulePort dataType="String" order="2" scale="40" precision="40" length="0" mandatory="true" portType="INPUT" name="IN_LAST_NAME"/>
    <RulePort dataType="String" inputPortName="IN_FIRST_NM" associatedRuleName="JH_MR_First_Name" order="5" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_FIRST_NM"/>
    <RulePort dataType="String" order="10" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(FINAL_FRST_MID_NM,' ')" name="FINAL_NAME"/>
    <RulePort dataType="String" order="8" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(IN_NAME,' ')>1,UPPER(CONCAT(FINAL_FRST_NM,MASK_MID_INITIAL)),UPPER(FINAL_FRST_NM))" name="FINAL_FRST_MID_NM"/>
    <RulePort dataType="String" order="15" scale="255" precision="255" length="0" mandatory="true" portType="OUTPUT" expression="FINAL_FULL_NAME" name="OUT_FULL_NAME"/>
    <RulePort dataType="String" inputPortName="IN_LAST_NAME" associatedRuleName="JH_MR_Last_Name" order="9" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_LAST_NAME"/>
    <RulePort dataType="String" order="7" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(IN_NAME,' ')>1,CONCAT(MASK_FIRST_NM,' '),MASK_FIRST_NM)" name="FINAL_FRST_NM"/>
    <RulePort dataType="String" order="11" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="UPPER(CONCAT(FINAL_NAME,MASK_LAST_NAME))" name="FINAL_FULL_NAME"/>
    <RulePort dataType="String" order="0" scale="40" precision="40" length="0" mandatory="true" portType="INPUT" name="IN_NAME"/>
    <RulePort dataType="String" inputPortName="IN_MID_INITIAL" associatedRuleName="JH_MR_MIDDLE_INITIAL" order="6" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="MASK_MID_INITIAL"/>
