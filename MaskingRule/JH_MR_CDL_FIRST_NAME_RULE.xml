    <RulePort dataType="String" order="3" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(IN_NAME,' ')>1,SUBSTR(IN_NAME,1,INSTR(IN_NAME,' ')-1),IN_NAME)" name="IN_FIRST_NM"/>
    <RulePort dataType="String" order="13" scale="40" precision="40" length="0" mandatory="true" portType="OUTPUT" expression="FINAL_FRST_MID_NM" name="OUT_FIRST_NAME"/>
  </Rule>
