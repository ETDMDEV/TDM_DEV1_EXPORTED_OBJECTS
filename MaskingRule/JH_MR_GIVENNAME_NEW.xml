  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_GIVENNAME_NEW">
    <RulePort dataType="String" order="0" scale="20" precision="20" length="0" mandatory="true" portType="INPUT" name="GIVENNAME"/>
    <RulePort dataType="String" order="1" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(GIVENNAME))" name="Spacedeletion"/>
    <RulePort dataType="String" order="8" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(Spacelast,CONCAT(' ',Spacefirst))" name="concat"/>
    <RulePort dataType="String" order="5" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Spacedeletion,1,spaceposition-1)" name="extractlast"/>
    <RulePort dataType="String" order="4" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(Spacedeletion,spaceposition+1,length(Spacedeletion))))" name="extractfirst"/>
    <RulePort dataType="String" order="9" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(spaceposition>0,concat,Firstrule)" name="matchspace"/>
    <RulePort dataType="String" inputPortName="extractlast" associatedRuleName="JH_MR_First_Name" order="6" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Spacelast"/>
    <RulePort dataType="Numeric" order="2" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(Spacedeletion,' ',1,1)" name="spaceposition"/>
    <RulePort dataType="String" inputPortName="Spacedeletion" associatedRuleName="JH_MR_First_Name" order="3" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Firstrule"/>
    <RulePort dataType="String" inputPortName="extractfirst" associatedRuleName="JH_MR_First_Name" order="7" scale="20" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Spacefirst"/>
    <RulePort dataType="String" order="10" scale="20" precision="20" length="0" mandatory="true" portType="OUTPUT" expression="matchspace" name="output"/>
