  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="jh_mr_mador_organization_name" description="rule to mask the organization name by concatenating first name and last name">
    <RulePort dataType="String" order="13" scale="10" precision="16" length="0" mandatory="true" portType="OUTPUT" expression="final_f_name" name="out_f_name"/>
    <RulePort dataType="String" inputPortName="concat_org" associatedRuleName="JH_MR_Organization_Rule" order="9" scale="10" precision="36" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="masked_org_name"/>
    <RulePort dataType="String" order="10" scale="10" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF((chk_char=1 or chk_char_D=1) and record_type='I',UPPER(substr(masked_org_name,1,20)),UPPER(masked_L_name))" name="final_l_name"/>
    <RulePort dataType="String" inputPortName="F_name" associatedRuleName="JH_MR_First_Name" order="7" scale="10" precision="16" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="masked_F_name"/>
    <RulePort dataType="String" order="0" scale="10" precision="16" length="0" mandatory="true" portType="INPUT" name="F_name"/>
    <RulePort dataType="String" order="12" scale="10" precision="20" length="0" mandatory="true" portType="OUTPUT" expression="final_l_name" name="out_L_name"/>
    <RulePort dataType="String" order="2" scale="10" precision="10" length="0" mandatory="true" portType="INPUT" name="state_pass_back"/>
    <RulePort dataType="String" order="8" scale="10" precision="36" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(F_name,L_name)" name="concat_org"/>
    <RulePort dataType="Numeric" order="5" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(LTRIM(state_pass_back),'B')" name="chk_char"/>
    <RulePort dataType="String" order="1" scale="10" precision="20" length="0" mandatory="true" portType="INPUT" name="L_name"/>
    <RulePort dataType="String" inputPortName="L_name" associatedRuleName="JH_MR_Last_Name" order="6" scale="10" precision="20" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="masked_L_name"/>
    <RulePort dataType="String" order="3" scale="10" precision="1" length="0" mandatory="true" portType="INPUT" name="record_type"/>
    <RulePort dataType="Numeric" order="4" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(LTRIM(state_pass_back),'D')" name="chk_char_D"/>
    <RulePort dataType="String" order="11" scale="10" precision="16" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF((chk_char=1 or chk_char_D=1) and record_type='I',UPPER(substr(masked_org_name,21)),upper(masked_F_name))" name="final_f_name"/>
  </Rule>
