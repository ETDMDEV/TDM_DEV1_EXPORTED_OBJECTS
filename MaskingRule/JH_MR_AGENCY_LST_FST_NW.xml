  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_AGENCY_LST_FST_NW">
    <RulePort dataType="String" order="7" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(Upper_last,','),Upper_first)" name="Concat"/>
    <RulePort dataType="String" order="6" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="upper(REPLACESTR(1,First_rule,',',''))" name="Upper_first"/>
    <RulePort dataType="String" order="9" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(RPAD(Match,24,' '),',')" name="comma"/>
    <RulePort dataType="String" inputPortName="Last" associatedRuleName="JH_MR_Last_Name" order="2" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Last_rule"/>
    <RulePort dataType="String" order="0" scale="30" precision="30" length="0" mandatory="true" portType="INPUT" name="INPUT1"/>
    <RulePort dataType="String" order="3" scale="40" precision="40" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="upper(REPLACESTR(1,Last_rule,',',''))" name="Upper_last"/>
    <RulePort dataType="String" order="1" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(INPUT1,1,INSTR(INPUT1,',')-1)" name="Last"/>
    <RulePort dataType="String" order="8" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(SUBSTR(Concat,1,1)=',',REPLACESTR(1,Concat,SUBSTR(Concat,1,1),''),Concat)" name="Match"/>
    <RulePort dataType="String" order="4" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(INPUT1,INSTR(INPUT1,',',1,1),length(INPUT1))" name="First"/>
    <RulePort dataType="String" inputPortName="First" associatedRuleName="JH_MR_First_Name" order="5" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="First_rule"/>
    <RulePort dataType="String" order="10" scale="30" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(SUBSTR(INPUT1,25,1)=',',comma,Match)" name="Matchagain"/>
    <RulePort dataType="String" order="11" scale="30" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="Matchagain" name="OUTPUT1"/>
  </Rule>
