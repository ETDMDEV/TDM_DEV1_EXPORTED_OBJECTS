<?xml version='1.0' encoding='UTF-8'?>
<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_MM_DD_YYYY_OR_YYYYMMDD_FORMAT" description="DATE IN MM/DD/YYYY OR YYYYMMDD FORMAT">
    <RulePort dataType="String" order="0" scale="10" precision="10" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="String" order="8" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,1,4)" name="VAR_YY2"/>
    <RulePort dataType="String" order="1" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0 OR ISNULL(IN_DOB),'11/11/1111',IN_DOB)" name="VAR1"/>
    <RulePort dataType="String" order="12" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(INSTR(VAR1,'/')>0,CONCAT_DATE1,CONCAT_DATE2)" name="FINAL_DOB"/>
    <RulePort dataType="String" order="4" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(DECODE(TO_INTEGER(VAR_DD1),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00)),2,'0')" name="VAR_DD_MASK1"/>
    <RulePort dataType="String" order="5" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,1,2)" name="VAR_MM1"/>
    <RulePort dataType="String" order="2" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,4,2)" name="VAR_DD1"/>
    <RulePort dataType="String" order="13" scale="10" precision="10" length="0" mandatory="true" portType="OUTPUT" expression="IIF(LENGTH(LTRIM(RTRIM(IN_DOB)))=0 OR ISNULL(IN_DOB),IN_DOB,FINAL_DOB)" name="OUT_DOB"/>
    <RulePort dataType="String" order="11" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="VAR_YY2||VAR_MM2||VAR_DD_MASK2" name="CONCAT_DATE2"/>
    <RulePort dataType="String" order="9" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(DECODE(TO_INTEGER(VAR_DD2),01,11,02,12,03,13,04,14,05,15,06,16,07,17,08,18,09,19,10,20,11,21,12,22,13,23,14,24,15,25,16,26,17,27,18,28,19,01,20,02,21,03,22,04,23,05,24,06,25,07,26,08,27,09,28,10,29,21,30,22,31,23,00,00)),2,'0')" name="VAR_DD_MASK2"/>
    <RulePort dataType="String" order="3" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,7,2)" name="VAR_DD2"/>
    <RulePort dataType="String" order="7" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,5,2)" name="VAR_MM2"/>
    <RulePort dataType="String" order="6" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(VAR1,7,4)" name="VAR_YY1"/>
    <RulePort dataType="String" order="10" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="VAR_MM1||'/'||VAR_DD_MASK1||'/'||VAR_YY1" name="CONCAT_DATE1"/>
  </Rule>
</Metadata>