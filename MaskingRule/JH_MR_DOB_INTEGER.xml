<Metadata xmlns="http://www.informatica.com/ilm/pdmds/xml/model" type="Rule" version="10.4.1;null;32;20833">
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="true" name="JH_MR_DOB" description="JH_Random_Bluring_Masking" dataType="Date" maskingType="RANDOM">
    <RuleParameter name="errhandling_defser" value="true"/>
    <RuleParameter name="errhandling_const" value="false"/>
    <RuleParameter name="preprocess_expression"/>
    <RuleParameter name="excp_nullspaceconst" value="false"/>
    <RuleParameter name="errhandling_errout" value="false"/>
    <RuleParameter name="excp_nullspacelogerr" value="false"/>
    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="postprocess_expression"/>
    <RuleParameter name="excp_nullspaceignore" value="true"/>
    <RuleParameter name="errhandling_ignore" value="false"/>
    <RuleParameter name="randDate_minblur" value="10"/>
    <RuleParameter name="randDate_range" value="false"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
    <RuleParameter name="randDate_BlurUnit" value="Day"/>
    <RuleParameter name="randDate_maxblur" value="10"/>
    <RuleParameter name="randDate_blurring" value="true"/>
    <RuleParameter name="excp_nullspacetreat" value="false"/>
