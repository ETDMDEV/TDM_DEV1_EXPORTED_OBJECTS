  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_DOB_INTEGER" description="JH_DOB_Integer_Advanced_Rule">
    <RulePort dataType="String" order="3" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(VAR1= '00000000','11111111',VAR1)" name="VAR2"/>
    <RulePort dataType="Numeric" order="8" scale="0" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(TO_CHAR(IN_DOB))))=0,IN_DOB,VAR_DOB_INT)" name="VAR4"/>
    <RulePort dataType="String" order="2" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(LENGTH(LTRIM(RTRIM(VAR_DOB_CHAR)))=0,'11111111',VAR_DOB_CHAR)" name="VAR1"/>
    <RulePort dataType="Numeric" order="9" scale="0" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(IN_DOB=0,IN_DOB,VAR4)" name="VAR_MASKED_DOB_INT"/>
    <RulePort dataType="Numeric" order="7" scale="0" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_INTEGER(VAR3)" name="VAR_DOB_INT"/>
    <RulePort dataType="Numeric" order="0" scale="0" precision="10" length="0" mandatory="true" portType="INPUT" name="IN_DOB"/>
    <RulePort dataType="String" order="1" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(TO_CHAR(IN_DOB),8,'0')" name="VAR_DOB_CHAR"/>
    <RulePort dataType="Date" order="4" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DATE(VAR2,'YYYYMMDD')" name="VAR_DATE"/>
    <RulePort dataType="String" order="6" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_CHAR(VAR_DOB_MASK,'YYYYMMDD')" name="VAR3"/>
    <RulePort dataType="Numeric" order="10" scale="0" precision="10" length="0" mandatory="true" portType="OUTPUT" expression="VAR_MASKED_DOB_INT" name="OutINT_DOB"/>
    <RulePort dataType="Date" inputPortName="VAR_DATE" associatedRuleName="JH_MR_DOB" order="5" scale="8" precision="8" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="VAR_DOB_MASK"/>
  </Rule>
</Metadata>