  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_NAME_MASK_MADOR" description="Rule to mask individual and org name in mador file -- need to delete if not used">
    <RulePort dataType="String" order="1" scale="0" precision="10" length="0" mandatory="true" portType="INPUT" name="IN_STPASSBKINFO"/>
    <RulePort dataType="String" order="8" scale="0" precision="1" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(LTRIM(RTRIM(IN_STPASSBKINFO)),1,1)" name="VAR_CHK"/>
    <RulePort dataType="String" order="3" scale="0" precision="16" length="0" mandatory="true" portType="INPUT" name="IN_FNAME"/>
    <RulePort dataType="String" order="9" scale="0" precision="20" length="0" mandatory="true" portType="OUTPUT" expression="IIF(IN_RECTYP = 'I' AND (VAR_CHK = 'B' OR VAR_CHK = 'D'),SUBSTR(VAR_ORGNAME_MASK,1,20),VAR_LNAME_MASK)" name="OUT_LNAME"/>
    <RulePort dataType="String" order="0" scale="0" precision="1" length="0" mandatory="true" portType="INPUT" name="IN_RECTYP"/>
    <RulePort dataType="String" order="10" scale="0" precision="16" length="0" mandatory="true" portType="OUTPUT" expression="IIF(IN_RECTYP = 'I' AND (VAR_CHK = 'B' OR VAR_CHK = 'D'),SUBSTR(VAR_ORGNAME_MASK,21,16),VAR_FNAME_MASK)" name="OUT_FNAME"/>
    <RulePort dataType="String" order="4" scale="0" precision="36" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(IN_LNAME,IN_FNAME)" name="VAR_FULL_NAME"/>
    <RulePort dataType="String" order="2" scale="0" precision="20" length="0" mandatory="true" portType="INPUT" name="IN_LNAME"/>
    <RulePort dataType="String" inputPortName="IN_FNAME" associatedRuleName="JH_MR_First_Name" order="7" scale="0" precision="16" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="VAR_FNAME_MASK">
