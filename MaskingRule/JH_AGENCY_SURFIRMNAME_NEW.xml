    <RulePort dataType="String" order="13" scale="50" precision="50" length="0" mandatory="true" portType="OUTPUT" expression="match" name="Output"/>
    <RulePort dataType="String" order="5" scale="50" precision="50" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(SUBSTR(Spacedeletion,spaceposition+1,Length(Spacedeletion))))" name="extractfirst"/>
    <RulePort dataType="Numeric" order="3" scale="10" precision="10" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="INSTR(Spacedeletion,' ',1,1)" name="spaceposition"/>
    <RulePort dataType="String" inputPortName="Spacedeletion" associatedRuleName="JH_MR_Last_Name" order="4" scale="50" precision="50" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="lastrule"/>
    <RulePort dataType="String" order="9" scale="50" precision="50" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(Spacelast,CONCAT(' ',Spacefirst))" name="Concat"/>
    <RulePort dataType="String" order="0" scale="20" precision="20" length="0" mandatory="true" portType="INPUT" name="Givenname"/>
    <RulePort dataType="String" order="6" scale="50" precision="50" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(Spacedeletion,1,spaceposition-1)" name="extractlast"/>
    <RulePort dataType="String" order="1" scale="50" precision="50" length="0" mandatory="true" portType="INPUT" name="Surfirmname"/>
    <RulePort dataType="String" inputPortName="extractlast" associatedRuleName="JH_MR_Last_Name" order="7" scale="50" precision="50" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="Spacelast"/>
