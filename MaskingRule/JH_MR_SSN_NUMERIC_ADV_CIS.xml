    <RuleParameter name="excp_nullspacedefser" value="false"/>
    <RuleParameter name="keyStr_res_chk" value="true"/>
    <RuleParameter name="errhandling_logexcp" value="false"/>
  </Rule>
  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_MR_SSN_NUMERIC_ADV_CIS" description="CIS 10 DIGIT SSN">
    <RulePort dataType="Numeric" order="9" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(ISNULL(IN_SSN),IN_SSN,V_INTEGER)" name="M_SSN_FINAL"/>
    <RulePort dataType="String" order="3" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(V_TRIM,9,'0')" name="V_LPAD_9"/>
    <RulePort dataType="String" order="6" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(TO_INTEGER(LENGTH_VTRIM)=10,V_LPAD_10,V_LPAD_9)" name="LPAD_FINAL"/>
    <RulePort dataType="Numeric" order="0" scale="10" precision="30" length="0" mandatory="true" portType="INPUT" name="IN_SSN"/>
    <RulePort dataType="String" order="1" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(ISNULL(TO_CHAR(IN_SSN)),'999999999',TO_CHAR(IN_SSN))" name="V_SSN"/>
    <RulePort dataType="Numeric" order="4" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LENGTH(V_TRIM)" name="LENGTH_VTRIM"/>
    <RulePort dataType="String" order="5" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LPAD(V_TRIM,10,'0')" name="V_LPAD_10"/>
    <RulePort dataType="Numeric" order="8" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="TO_DECIMAL(V_MASK,TRUE)" name="V_INTEGER"/>
    <RulePort dataType="String" order="2" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="LTRIM(RTRIM(V_SSN)) " name="V_TRIM"/>
    <RulePort dataType="Numeric" order="10" scale="10" precision="30" length="0" mandatory="true" portType="OUTPUT" expression="M_SSN_FINAL" name="OUT_SSN"/>
    <RulePort dataType="String" inputPortName="LPAD_FINAL" associatedRuleName="JH_MR_SSN" order="7" scale="10" precision="30" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="V_MASK"/>
  </Rule>
</Metadata>