  <Rule xmlns="http://www.informatica.com/ilm/pdmds/xml/model" active="true" overideAble="false" name="JH_TRUECOMP_SHORTNAME" description="IF PN=1, SHORTNAME ELSE IF PN=2, DONT">
    <RulePort dataType="String" order="8" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(SUBSTR(MATCH,1,1)=',',REPLACESTR(1,MATCH,SUBSTR(MATCH,1,1),''),MATCH)" name="MATCHS"/>
    <RulePort dataType="String" order="6" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="CONCAT(CONCAT(LAST_RULE,', '),FIRST_RULE)" name="FULL"/>
    <RulePort dataType="String" order="7" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="IIF(PAGENUMBER=1,FULL,SHORT_INPUT)" name="MATCH"/>
    <RulePort dataType="String" order="9" scale="255" precision="255" length="0" mandatory="true" portType="OUTPUT" expression="MATCHS" name="Output_data"/>
    <RulePort dataType="String" order="4" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="RTRIM(LTRIM(SUBSTR(SHORT_INPUT,INSTR(SHORT_INPUT,',',1,1)+1,length(SHORT_INPUT))))" name="FIRST"/>
    <RulePort dataType="String" order="1" scale="255" precision="255" length="0" mandatory="true" portType="INPUT" name="SHORT_INPUT"/>
    <RulePort dataType="String" order="2" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" expression="SUBSTR(SHORT_INPUT,1,INSTR(SHORT_INPUT,',')-1)" name="LAST"/>
    <RulePort dataType="String" inputPortName="FIRST" associatedRuleName="JH_MR_First_Name" order="5" scale="255" precision="255" length="0" mandatory="true" portType="LOCAL_VARIABLE" name="FIRST_RULE">
